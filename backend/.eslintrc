{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "object-curly-newline": ["error", { "ImportDeclaration": "never" }],
    "no-console": "off",
    "func-names": ["error", "never"],
    "no-useless-escape": "off",
    "no-shadow": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "^next" }]
  }
}